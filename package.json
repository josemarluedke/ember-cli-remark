    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.1",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^7.11.1",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "qunit-dom": "^1.1.0",
    "remark-autolink-headings": "^6.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-slug": "^6.0.0",
    "tailwindcss": "^1.2.0"
